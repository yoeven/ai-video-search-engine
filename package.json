    "@fontsource/inter": "^5.0.15",
    "@nikolovlazar/chakra-ui-prose": "^1.2.1",
    "@supabase/supabase-js": "^2.38.5",
    "@xenova/transformers": "^2.11.0",
    "dayjs": "^1.11.10",
    "entities": "^4.5.0",
    "framer-motion": "^10.16.5",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-ws": "^5.14.2",
    "jose": "^5.1.1",
    "langchain": "^0.0.197-rc.1",
    "next": "13.3.4",
    "next-seo": "^6.4.0",
